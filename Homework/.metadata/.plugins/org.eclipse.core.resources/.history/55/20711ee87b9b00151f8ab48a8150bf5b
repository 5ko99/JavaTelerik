import java.math.BigDecimal;

import BankAcc.*;
import customers.*;

public class BankStartup {
	@SuppressWarnings({ "null", "unused" })
	public static void main(String[] args){
	BigDecimal balance = new BigDecimal(56.25);
	BigDecimal interestRate = new BigDecimal(2.215);
	
	Companie companie = new Companie("Gen'Soft","Montana");
	Individual individual = new Individual("Petko","Sofia");
	
	Deposit deposit = new Deposit(balance,interestRate,companie);
	
	balance= new BigDecimal(50301.21);
	interestRate = new BigDecimal(4.248);
	
	Loan loan = new Loan(balance, interestRate, companie);
	
	System.out.println(loan.getBalance());
	loan.deposit(balance);
	System.out.println(loan.getBalance());
	
	}
}
