package animals;

public class cat {
	private static int myid=0;
	private static String name;
	private static int age;
	private int id;
	
	public cat(String name,int age){
		this.age = age;
		this.name =name;
	}
	
	public static void getinfo(){
		System.out.println(name);
		System.out.println(age);
		
	}
}
