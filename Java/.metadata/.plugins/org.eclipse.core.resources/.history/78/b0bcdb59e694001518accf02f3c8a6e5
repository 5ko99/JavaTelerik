
public class Anonim implements  IrandomProvider {
	public IrandomProvider IrandomProvider= new IrandomProvider(){
		public double Get(){
			return Math.random();
		}
	};
	public static void main(String[] args){
		IrandomProvider a = new IrandomProvider() {
		};
		double b = a.Get();
		for (int i = 0; i < 20; i++) {
			System.out.println(b);
		}
	}
}
