package BankAcc;
import java.math.BigDecimal;

import customers.Customer;

public class Deposit extends Bank {
	
	
	
	public Deposit(BigDecimal balance, BigDecimal interestRate, Customer customer) {
		super(balance, interestRate, customer);
		// TODO Auto-generated constructor stub
	}

	public  void withDraw(BigDecimal sum){
		if(sum.compareTo(getBalance())>0){
			balance.subtract(sum);
		}
		else {
			System.out.println("You don't have enought mony to Whith Draw");
		}
	}
}
