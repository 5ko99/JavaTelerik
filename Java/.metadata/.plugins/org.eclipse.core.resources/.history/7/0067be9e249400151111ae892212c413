package Geometry;

public class Square extends Rectangle {
   private int size;
	public Square(int x, int y,int size) {
		super(x,y,size);
		this.size=size;
	}
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	public double getArea() {
		// TODO Auto-generated method stub
		return size*size;
	}

}
